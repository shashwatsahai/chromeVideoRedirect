{
    "name": "googleRedirect",
    "version":"1.0",
    "manifest_version":2,
    "content_scripts": [
        {
            "matches":["<all_urls>"],
            "run_at": "document_end",
            "js": ["content.js"]
        }
    ],
    "permissions": [
        "webRequest",
        "https://www.google.com/*",
        "*https://www.google.com*",
        "webRequestBlocking"
      ]
}